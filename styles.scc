h1 {
  font-family: "Source Serif Pro", serif;
  color: #7bad99;
}

h2 {
  font-family: "Monserrat", sans-serif;
  color: #9baac1;
}

h3 {
  font-family: "Merriweather", serif;
  color: #5e7597;
}

h1 {
  font-size: 2.4rem;
  font-weight: 300;
  line-height: 1.2;
}

h2 {
  font-size: 1.8rem;
  font-weight: bold;
  text-transform: uppercase;
}

h3 {
  font-size: 1.5rem;
  font-weight: bold;
  text-transform: uppercase;
}

section > h2::before {
  color: #9bc1b2;
}

p {
  font-size: 1.4rem;
  line-height: 1.4;
  color: #5e7597;
}

ul {
  list-style-type: none;
  text-decoration: none;
}

li {
  list-style-type: none;
  text-decoration: none;
  color: #4b5d78;
}

a {
  color: #97805e;
  text-decoration: none;
  list-style-type: none;
  text-decoration: none;
}

*::before,
*::after {
  margin: 0;
  padding: 0;
  box-sizing: inherit;
}

